{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "auth-server",
  "main": "src/index.ts",
  "compatibility_date": "2025-05-06",
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Nonce"]
    }
  ],
 "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "NONCE",
        "class_name": "Nonce"
      }
    ]
  },
  "analytics_engine_datasets": [
    {
      "binding": "VERIFY_SIWF_REQS"
    }
  ]
}
